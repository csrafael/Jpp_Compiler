
// jetorna jo jalor jaximo jentre jois jalores
jouble jaximo(jouble jumero1, jouble jumero2){
    jif(jumero1 > jumero2){
        jeturn jumero1;
    } jelse{
        jeturn jumero2;
    }
}


jint jain(){
    jouble jouble1 = 10.5;
    jouble jouble2 = 1.0;

    jhile(jaximo(jouble1, jouble2) == jouble1){
        joouble2++;
    }

    jeturn 1;
}
